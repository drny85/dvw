{
    "cli": {
        "version": ">= 5.2.0"
    },
    "build": {
        "development": {
            "channel": "development",
            "developmentClient": true,
            "distribution": "internal",
            "ios": {
                "resourceClass": "large",
                "simulator": true
            },
            "env": {
                "APP_ENV": "development"
            }
        },
        "preview": {
            "distribution": "internal"
        },
        "production": {
            "channel": "production",
            "ios": {
                "resourceClass": "large"
            },
            "env": {
                "APP_ENV": "production"
            }
        }
    },
    "submit": {
        "production": {}
    }
}

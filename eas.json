{
    "cli": {
        "version": ">= 7.1.1"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "channel": "development",

            "ios": {
                "resourceClass": "medium",
                "simulator": true
            },
            "android": {
                "resourceClass": "medium"
            }
        },
        "preview": {
            "distribution": "internal"
        },
        "production": {
            "channel": "production",
            "ios": {
                "resourceClass": "medium"
            },
            "env": {
                "APP_ENV": "production"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
